@model List<InsignisIllustrationGenerator.Models.IllustrationListViewModel>
@{
    ViewData["Title"] = "PreviousIllustration";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Previous Illustration</h1>
<div class="middle-section">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="right"><a href="#" class="btn btn-primary">Generate CSV</a></div>
            </div>
            <div class="col-lg-12 col-sm-12">
                <div class="illustration-generator">
                    <h4 class="heading-1">Search By:</h4>
                    <div class="gray-bg">

                        <form>
                            <div class="form-row">
                                <div class="form-group col-lg-4">
                                    <label for="inputEmail4">Advisor Name</label>
                                    <input type="email" class="form-control" id="" placeholder="Advisor Name">
                                </div>
                                <div class="form-group col-lg-4">
                                    <label for="inputPassword4">Client Name</label>
                                    <input type="email" class="form-control" id="" placeholder="Client Name">
                                </div>
                                <div class="form-group col-lg-4">
                                    <label for="inputPassword4">Company Name</label>
                                    <input type="email" class="form-control" id="" placeholder="Company Name">
                                </div>
                                <div class="form-group col-lg-4">
                                    <label for="inputPassword4">Illustration Unique Reference</label>
                                    <input type="email" class="form-control" id="" placeholder="Illustration Unique Reference">
                                </div>
                                <div class="form-group col-lg-4">
                                    <label for="inputPassword4">From Date</label>
                                    <input type="email" class="form-control" id="" placeholder="dd-mm-yyyy">
                                </div>
                                <div class="form-group col-lg-4">
                                    <label for="inputPassword4">To Date</label>
                                    <input type="email" class="form-control" id="" placeholder="dd-mm-yyyy">
                                </div>
                            </div>
                            <div class="col-lg-12 center">
                                <button type="submit" class="btn btn-primary">Search</button>
                                <button type="submit" class="btn btn-primary">Reset</button>
                            </div>
                        </form>

                    </div>

                </div>
            </div>
        </div>
        <div class="clear1"></div>
        @Html.Partial("_IllustrationList", Model)

    </div>

</div>